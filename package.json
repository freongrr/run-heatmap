    "@types/pouchdb": "6.4.0",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "css-loader": "6.7.1",
    "html-webpack-plugin": "5.5.0",
    "sass": "1.53.0",
