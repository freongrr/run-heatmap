{
  "name": "run-heatmap",
  "version": "1.0.0",
  "description": "Run Heatmap",
  "main": "index.js",
  "scripts": {
    "client:dev": "webpack serve --open --config webpack.dev.js",
    "client:build": "rm -rf ./dist && webpack --config webpack.prod.js",
    "client:analyze": "ANALYZE_BUNDLE=1 npm run build",
    "server:build": "rm -rf ./dist-server && tsc --build tsconfig.server.json",
    "server:start": "node dist-server/server/index.js",
    "all": "npm run client:build && npm run server:build && npm run server:start"
  },
  "keywords": [],
  "author": "Fabien Cortina <fabien.cortina@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mapbox-gl": "2.3.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.7",
    "@types/xmldom": "^0.1.31",
    "css-loader": "5.2.6",
    "html-webpack-plugin": "5.3.1",
    "sass": "1.34.1",
    "sass-loader": "12.1.0",
    "style-loader": "2.0.0",
    "ts-loader": "9.2.3",
    "typescript": "4.3.2",
    "webpack": "5.38.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@turf/turf": "6.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "mapbox-gl": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "xmldom": "^0.6.0"
  }
}
