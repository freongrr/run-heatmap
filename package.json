{
  "name": "run-heatmap",
  "version": "1.0.0",
  "description": "Run Heatmap",
  "main": "index.js",
  "scripts": {
    "client:dev": "webpack serve --open --config webpack.dev.js",
    "client:build": "rm -rf ./dist && webpack --config webpack.prod.js",
    "client:analyze": "ANALYZE_BUNDLE=1 npm run build",
    "server:build": "rm -rf ./dist-server && tsc --build tsconfig.server.json",
    "server:start": "node dist-server/server/index.js",
    "all": "npm run client:build && npm run server:build && npm run server:start"
  },
